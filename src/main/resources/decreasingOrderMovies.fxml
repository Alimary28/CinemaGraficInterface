<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.DecreasingMovieController">
    <VBox alignment="CENTER">
        <Label text="Decreasing oredr movies"/>
    </VBox>
    <HBox alignment="CENTER">
        <TableView fx:id="tableViewMovies" editable="true" prefHeight="400.0" prefWidth="481.0">
            <columns>
                <TableColumn fx:id="tableColumnMovieTitle" prefWidth="130.0" text="Title">
                    <cellValueFactory>
                        <PropertyValueFactory property="title" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="tableColumnBookings" prefWidth="90.0" text="Bookings">
                    <cellValueFactory>
                        <PropertyValueFactory property="bookings" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </HBox>
</HBox>
